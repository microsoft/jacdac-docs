  "scripts": {
    "setup": "git submodule update --init --recursive && git pull && yarn install --frozen-lockfile",
    "postinstall": "(cd jacdac-ts && yarn dist) && (cd jacscript/compiler && yarn build) && yarn buildml4f && yarn buildworkers",
    "pullsubmodules": "git submodule update --init --recursive",
    "lint": "node node_modules/eslint/bin/eslint.js src/**/*.ts src/**/*.tsx",
    "clean": "node node_modules/gatsby/cli.js clean",
    "blocklymedia": "mkdir -p static/blockly && cp -r -f node_modules/blockly/media static/blockly",
    "predistdocs": "yarn clean && yarn blocklymedia && cd jacdac-ts/tools && node ./prepare.js",
    "distdocs": "node ./node_modules/gatsby/cli.js build --prefix-paths --no-uglify --verbose",
    "tools": "npx http-server . -c-1",
    "watchdocs": "node node_modules/gatsby/cli.js develop",
    "prettier": "prettier --write src/**/*.ts src/**/*.tsx pxt-jacdac/**/*.ts",
    "develop": "node node_modules/concurrently/dist/bin/concurrently.js --kill-others \"npm:watch*\"",
    "docsbuild": "node node_modules/gatsby/cli.js build",
    "docsserve": "node node_modules/gatsby/cli.js serve",
    "dockerdocs": "node node_modules/gatsby/cli.js develop --host:0.0.0.0",
    "buildspecs": "cd jacdac-ts && yarn buildspecs && yarn prettier && cd .. && cp -r jacdac-ts/jacdac-spec/services/generated/makecode/* pxt-jacdac && cp -r jacdac-ts/jacdac-spec/services/generated/python/* jacdac-python/jacdac && rm jacdac-dotnet/Jacdac/Constants/*.cs && cp -r jacdac-ts/jacdac-spec/services/generated/cs/* jacdac-dotnet/Jacdac && yarn prettier && cd pxt-jacdac && sh update-clients.sh",
    "buildml4f": "cd ml4f && yarn",
    "buildpxt": "cd pxt-jacdac && sh mk.sh",
    "buildmodels": "cd static/models && sh mk.sh",
    "builddocsts": "node node_modules/typescript/bin/tsc -b src",
    "buildworkercsv": "cd src/workers/csv && node ../../../node_modules/microbundle/dist/cli.js build -f modern --no-compress --types --ts-config tsconfig.json",
    "buildworkerdata": "cd src/workers/data && node ../../../node_modules/microbundle/dist/cli.js build -f modern --no-compress --types --ts-config tsconfig.json",
    "buildworkertf": "cd src/workers/tf && node ../../../node_modules/microbundle/dist/cli.js build -f modern --no-compres --types --ts-config tsconfig.json",
    "buildworkervm": "cd src/workers/vm && node ../../../node_modules/esbuild/bin/esbuild --bundle vm-worker.ts --outdir=dist/node_modules --sourcemap",
    "buildworkerjacscript": "cd src/workers/jacscript && node ../../../node_modules/esbuild/bin/esbuild --bundle jacscript-worker.ts --outdir=dist/node_modules --sourcemap",
    "buildworkercad": "cd src/workers/cad && node ../../../node_modules/microbundle/dist/cli.js build -f modern --no-compress --types --ts-config tsconfig.json",
    "buildworkers": "yarn buildworkercsv && yarn buildworkerdata && yarn buildworkertf && yarn buildworkervm && yarn buildworkercad && yarn buildworkerjacscript",
    "watchdocsts": "node node_modules/typescript/bin/tsc -b src --watch",
    "watchworkercsv": "cd src/workers/csv && node ../../../node_modules/microbundle/dist/cli.js watch -f modern --no-compress --types --ts-config tsconfig.json",
    "watchworkerdata": "cd src/workers/data && node ../../../node_modules/microbundle/dist/cli.js watch -f modern --no-compress --types --ts-config tsconfig.json",
    "watchworkertf": "cd src/workers/tf && node ../../../node_modules/microbundle/dist/cli.js watch -f modern --no-compress --types --ts-config tsconfig.json",
    "watchworkervm": "cd src/workers/vm && node ../../../node_modules/esbuild/bin/esbuild --bundle vm-worker.ts --outdir=dist/node_modules --sourcemap --watch",
    "watchworkerjacscript": "cd src/workers/jacscript && node ../../../node_modules/esbuild/bin/esbuild --bundle jacscript-worker.ts --outdir=dist/node_modules --sourcemap --watch",
    "watchworkercad": "cd src/workers/cad && node ../../../node_modules/microbundle/dist/cli.js watch -f modern --no-compress --types --ts-config tsconfig.json",
    "watchts": "cd jacdac-ts && yarn watch",
    "watchjacscript": "cd jacscript/compiler && yarn watch",
    "buildcssamples": "cd jacdac-dotnet && snippets.bat",
    "buildjacscript": "make -C jacscript/ && yarn buildworkervm && yarn buildworkerjacscript"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/jacdac-docs"
